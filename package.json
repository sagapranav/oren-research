{
  "name": "oren-flash",
  "version": "1.0.0",
  "type": "module",
  "description": "Multi-agent research system with streaming UI",
  "scripts": {
    "dev:backend": "tsx watch --env-file=.env backend/server.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "build:backend": "tsc -p backend",
    "build:frontend": "cd frontend && npm run build",
    "build": "npm run build:backend && npm run build:frontend",
    "start": "node --env-file=.env dist/backend/server.js",
    "start:dev": "tsx --env-file=.env backend/server.ts",
    "type-check": "tsc -p backend --noEmit",
    "install:all": "npm install && cd frontend && npm install",
    "setup": "cp .env.example .env && cd frontend && cp .env.local.example .env.local && echo 'Setup complete! Edit .env files with your API keys.'",
    "test-models": "tsx --env-file=.env test-report-models.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.50",
    "@ai-sdk/openai": "^2.0.74",
    "@e2b/code-interpreter": "^1.0.4",
    "@openrouter/ai-sdk-provider": "^1.2.7",
    "@supabase/supabase-js": "^2.86.0",
    "ai": "^5.0.104",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "exa-js": "^1.0.12",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "marked": "^15.0.12",
    "pdfmake": "^0.2.20",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/pdfmake": "^0.2.12",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
